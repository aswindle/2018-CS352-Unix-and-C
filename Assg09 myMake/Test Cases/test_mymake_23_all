
all: libcs352.a test_cs352 

libcs352.a : cs352.o
	echo "What a nice makefile"
ar rsc $@ $<

cs352.o : cs352.c cs352.h
	gcc -Wall -c $< -o $@
	ls

test_cs352 : test_cs352.o libcs352.a 
	gcc test_cs352.o -L. -lcs352 -o $@
	pwd
ls 
	echo "Done now"

test_cs352.o : test_cs352.c cs352.h
	gcc -Wall -c $< -o $@
	ls test_cs352.o 


